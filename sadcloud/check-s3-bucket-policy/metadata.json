{
  "policy": {
    "name": "check-s3-bucket-policy",
    "resource": "s3",
    "description": "Check for S3 buckets with public access in their policy",
    "filters": [
      {
        "type": "has-statement",
        "statements": [
          {
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:*"
          }
        ]
      }
    ],
    "actions": [
      {
        "type": "remove-statements",
        "statement_ids": [
          "PublicAccess"
        ]
      },
